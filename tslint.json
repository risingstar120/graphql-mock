{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "ordered-imports": false,
    "no-console": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ]
  }
}
