{
  "name": "graphql-mock",
  "version": "0.4.1",
  "description": "GraphQL endpoint mockery library for testing",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepare": "npm build",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MadRabbit/graphql-mock.git"
  },
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "dependencies": {
    "apollo-link-schema": "^1.1.0",
    "graphql-tools": "^3.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.0",
    "@types/jest": "^22.2.3",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.11",
    "@types/node-fetch": "^1.6.9",
    "@types/react": "^16.3.13",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-link-http": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./test/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*_test.(ts|tsx)"
    ]
  }
}
