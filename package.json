{
  "name": "graphql-mock",
  "version": "1.0.0",
  "description": "GraphQL endpoint mockery library for testing",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepare": "npm build",
    "test": "jest -c test/jest.config.json"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MadRabbit/its-alive.git"
  },
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "dependencies": {
    "apollo-link-schema": "^1.0.2",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.18.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.11.8",
    "@types/jest": "^22.0.1",
    "@types/nock": "^9.1.1",
    "@types/node": "^9.3.0",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^16.0.34",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-http": "^1.3.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "graphql-tag": "^2.6.1",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.0.0",
    "node-fetch": "^1.7.3",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.2"
  }
}
