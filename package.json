{
  "name": "graphql-mock",
  "version": "1.1.1",
  "description": "GraphQL endpoint mockery library for testing",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepare": "npm build",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MadRabbit/graphql-mock.git"
  },
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "dependencies": {
    "apollo-link-schema": "^1.1.1",
    "fast-deep-equal": "^2.0.1",
    "graphql-tools": "^4.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.1",
    "@types/jest": "^23.3.3",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.11.4",
    "@types/node-fetch": "^2.1.2",
    "@types/react": "^16.4.14",
    "apollo-cache-inmemory": "^1.2.10",
    "apollo-client": "^2.4.2",
    "apollo-link-http": "^1.5.5",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "husky": "^1.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./test/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*_test.(ts|tsx)"
    ]
  }
}
