{
  "name": "graphql-mock",
  "version": "1.1.2",
  "description": "GraphQL endpoint mockery library for testing",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "prepare": "npm run build",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MadRabbit/graphql-mock.git"
  },
  "author": "Nikolay Nemshilov",
  "license": "MIT",
  "dependencies": {
    "apollo-link-schema": "^1.2.2",
    "fast-deep-equal": "^2.0.1",
    "graphql-tools": "^4.0.4"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.13",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/react": "^16.8.17",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./test/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*_test.(ts|tsx)"
    ]
  }
}
